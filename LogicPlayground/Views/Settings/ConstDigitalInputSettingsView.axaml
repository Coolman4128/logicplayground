<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:vm="using:LogicPlayground.ViewModels.LogicBlocks.Inputs"
             mc:Ignorable="d" d:DesignWidth="300" d:DesignHeight="400"
             x:Class="LogicPlayground.Views.Settings.ConstDigitalInputSettingsView"
             x:DataType="vm:ConstDigitalInputViewModel">
    
    <StackPanel Margin="10" Spacing="10">
        <TextBlock Text="Const Digital Input Settings" FontSize="16" FontWeight="Bold" />
        
        <StackPanel Spacing="5">
            <TextBlock Text="Value:" />
            <CheckBox IsChecked="{Binding Value}" Content="High/Low" />
        </StackPanel>
    </StackPanel>
</UserControl>
